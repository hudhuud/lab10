def DigitCount(K):
    if K<0:
        return "Число К отрицательное"
    s=1
    while K//10!=0:
        s+=1
        K=K//10
    return s

print(DigitCount(1234))


def DigitCount(k):
    i=0
    while k != 0:
        k //= 10
        i+=1
    return i
a = int(input())
b = int(input())
c = int(input())
d = int(input())
e = int(input())
print(DigitCount(a),DigitCount(b),DigitCount(c),DigitCount(d),DigitCount(e), sep=', ')
